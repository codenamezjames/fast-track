{
  "name": "fast-track",
  "version": "2.0.0",
  "description": "Personal fitness and lifestyle tracking PWA - Complete rewrite with AdonisJS v6 + Vue 3 + PostgreSQL",
  "private": true,
  "workspaces": [
    "packages/api",
    "packages/web"
  ],
  "scripts": {
    "dev:api": "npm run dev --workspace=packages/api",
    "dev:web": "npm run dev --workspace=packages/web",
    "dev": "npm-run-all --parallel dev:api dev:web",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "up": "docker compose up -d",
    "down": "docker compose down",
    "logs": "docker compose logs -f",
    "rebuild": "docker compose down && docker compose up -d --build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "fitness",
    "tracking",
    "pwa",
    "adonisjs",
    "vue3",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
