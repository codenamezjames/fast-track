{
  "name": "fast-track",
  "version": "0.0.1",
  "description": "FastTrack - A calorie tracker with intermittent fasting features",
  "productName": "FastTrack",
  "author": "James Harrington <jamesdonaldharrington@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --reporter=json --outputFile=test-results.json",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "build:analyze": "quasar build --analyze",
    "build:prod": "quasar build --mode production",
    "serve:pwa": "quasar serve dist/pwa --port 3000",
    "deploy": "./deploy.sh",
    "postinstall": "quasar prepare",
    "perf:analyze": "npm run build:analyze",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "perf:audit": "npm run build:prod && npm run perf:lighthouse",
    "docs:generate": "jsdoc -c jsdoc.config.js",
    "docs:serve": "http-server docs -p 8080 -o",
    "docs:watch": "jsdoc -c jsdoc.config.js --watch",
    "prepare": "husky install",
    "lint:fix": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\" --fix",
    "format:check": "prettier --check \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format:fix": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "quality:check": "npm run lint && npm run format:check && npm run test:run",
    "quality:fix": "npm run lint:fix && npm run format:fix",
    "setup": "node scripts/dev-setup.js",
    "analyze": "node scripts/analyze-bundle.js",
    "dev:clean": "rm -rf node_modules package-lock.json && npm install",
    "dev:reset": "npm run dev:clean && npm run setup"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "appwrite": "^18.1.1",
    "chart.js": "^4.5.0",
    "dexie": "^4.0.11",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.4.18",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@pinia/testing": "^1.0.2",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@quasar/vite-plugin": "^1.10.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^3.2.4",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "husky": "^8.0.3"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
